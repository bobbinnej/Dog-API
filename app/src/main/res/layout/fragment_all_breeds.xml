    android:id="@+id/frameBreed"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AllBreeds">

    <!-- TODO: Update blank fragment layout -->
    <androidx.recyclerview.widget.RecyclerView
